<html>
<head>
</head>
<body>
<div id="header">

</div>
<div id="main">
  <div id="side-bar">
  </div>
  <div id="main-body">
    <% histories.each do |ch_id, history| %>
        <div>
          <%= channels.find{ |c| c['id'] == ch_id }['name'] %>
        </div>
        <% history['messages'].each do |m| %>
            <div><%= user_names[m['user']] %></div>
            <div><%= m['text'] %></div>
        <% end %>
    <% end %>
  </div>
</div>
</body>
</html>